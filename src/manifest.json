{
    "manifest_version": 3,
    "name": "Password Manager",
    "version": "1.0",
    "description": "A Chrome extension to manage websites, environments, and account passwords.",
    "permissions": ["storage", "tabs", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon16.png",
        "128": "icons/icon16.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon16.png",
      "128": "icons/icon16.png"
    }
  }